{
  "version": "1.0.0-beta1-*",

  "packOptions": {
    "description": "ASP.NET Core security middleware enabling Salesforce authentication.",
    "authors": [ "Andrew Mattie" ],
    "owners": [ "KÃ©vin Chalet", "Jerrie Pelser" ],

    "projectUrl": "https://github.com/aspnet-contrib/AspNet.Security.OAuth.Providers",
    "iconUrl": "https://avatars3.githubusercontent.com/u/7998081?s=64",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0.html",

    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet-contrib/AspNet.Security.OAuth.Providers"
    },

    "tags": [
      "aspnetcore",
      "authentication",
      "oauth",
      "salesforce",
      "security"
    ]
