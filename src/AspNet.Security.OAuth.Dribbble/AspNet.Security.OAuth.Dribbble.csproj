<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Authors>Hayden Hancock</Authors>
    <Company>Hayden Hancock</Company>
    <Product>aspnet-contrib</Product>
    <Description>ASP.NET Core security middleware enabling Dribbble authentication.</Description>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0.html</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/aspnet-contrib/AspNet.Security.OAuth.Providers</PackageProjectUrl>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/7998081?s=64</PackageIconUrl>
    <RepositoryUrl>git://github.com/aspnet-contrib/AspNet.Security.OAuth.Providers</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>aspnetcore;authentication;dribbble;oauth;security</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\shared\AspNet.Security.OAuth.Extensions\ClaimsExtensions.cs" Link="ClaimsExtensions.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.OAuth" Version="1.0.0" />
  </ItemGroup>

</Project>
