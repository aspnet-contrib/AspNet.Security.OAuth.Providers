{
  "$schema": "https://raw.githubusercontent.com/justeat/httpclient-interception/master/src/HttpClientInterception/Bundles/http-request-bundle-schema.json",
  "items": [
    {
      "uri": "https://connect.deezer.com/oauth/access_token.php?app_id=my-client-id&secret=my-client-secret&code=a6ed8e7f-471f-44f1-903b-65946475f351&output=json",
      "method": "GET",
      "contentFormat": "json",
      "contentJson": {
        "access_token": "secret-access-token",
        "token_type": "access",
        "refresh_token": "secret-refresh-token",
        "expires_in": "300"
      }
    },
    {
      "uri": "https://api.deezer.com/user/me?access_token=secret-access-token",
      "contentFormat": "json",
      "contentJson": {
        "id": "my-id",
        "name": "John Smith",
        "email": "john@john-smith.local"
      }
    }
  ]
}
